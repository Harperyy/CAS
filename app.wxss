/**app.wxss**/
@import 'weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.bc_green{
  background-color: rgb(99, 184, 241);
}
.bc_red{
  background-color: #a494dd;
}
.bc_blue{
  background-color: #10AEFF;
}
.bc_yellow{
  background-color: #FFBE00;
}
.bc_gray{
  background-color: #C9C9C9;
}

